# Cloudflare Turnstile (AC6.5 - Optional, leave empty to disable)
TURNSTILE_SECRET_KEY=
TURNSTILE_SITE_KEY=

# Disposable Email Blocking (AC6.7 - Optional)
BLOCK_DISPOSABLE_EMAILS=false
